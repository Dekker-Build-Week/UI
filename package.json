{
  "name": "material-dashboard-react",
  "version": "1.8.0",
  "description": "Material Dashboard React. Coded by Creative Tim",
  "private": false,
  "main": "dist/index.js",
  "engines": {
    "node": "12.10.0"
  },
  "dependencies": {
    "@material-ui/core": "4.3.2",
    "@material-ui/icons": "4.2.1",
    "axios": "^0.19.0",
    "babel-jest": "^24.9.0",
    "carousel": "^0.0.3",
    "classnames": "2.2.6",
    "jest-cli": "facebook/jest#0.5.x",
    "perfect-scrollbar": "1.4.0",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-chartist": "0.13.3",
    "react-dom": "16.9.0",
    "react-responsive": "^8.0.1",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.1.0",
    "react-slick": "^0.25.2",
    "slick-carousel": "^1.8.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageOnlyFrom": {
      "app/components": true
    },
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "react"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "jest",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "build-package-css": "cp src/assets/css/material-dashboard-react.css dist/material-dashboard-react.css",
    "build-package": "npm run build-package-css && babel src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/material-dashboard-react.git"
  },
  "keywords": [],
  "author": "Creative Tim <hello@creative-tim.com> (https://www.creative-tim.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/creativetimofficial/material-dashboard-react/issues"
  },
  "homepage": "https://creativetimofficial.github.io/material-dashboard-react/#/dashboard",
  "optionalDependencies": {
    "@types/googlemaps": "3.37.3",
    "@types/markerclustererplus": "2.1.33",
    "ajv": "6.10.2",
    "typescript": "3.5.3"
  },
  "devDependencies": {
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "prettier": "1.18.2",
    "react-test-renderer": "^16.9.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": []
  }
}
